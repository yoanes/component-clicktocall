<?xml version="1.0" encoding="UTF-8"?>
<bs:bundlesConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:bs="http://mobile.sensis.com.au/build-scripts"
    xsi:schemaLocation="http://mobile.sensis.com.au/build-scripts au.com.sensis.mobile.bundles.xsd">
    
    <bundles basename="clicktocall-component" package="clicktocall">
        <bundle name="deviceInvariant" abstract="true">
            <fileset dir="${src.javascript.dir}/clicktocall/device-invariant">
            </fileset>
        </bundle>
        <bundle name="default" parent="deviceInvariant">
            <fileset dir="${src.javascript.dir}/clicktocall/device-variant/default">
            </fileset>
        </bundle>
        <bundle name="delayedAjax" parent="deviceInvariant">
            <fileset dir="${src.javascript.dir}/clicktocall/device-variant/delayedAjax">
            </fileset>
        </bundle>
    </bundles>
</bs:bundlesConfig>
