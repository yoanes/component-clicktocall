    <identifiedDeviceConfig>
        <deviceId>Apple-iPhone</deviceId>
        
        <deviceConfig xsi:type="clicktocall:DeviceConfig">
            <enableImmediateClickToCall>true</enableImmediateClickToCall>
        </deviceConfig>
    </identifiedDeviceConfig>    
    
    <identifiedDeviceConfig>
        <deviceId>Android-OS</deviceId>
        
        <deviceConfig xsi:type="clicktocall:DeviceConfig">
            <enableImmediateClickToCall>true</enableImmediateClickToCall>
        </deviceConfig>
    </identifiedDeviceConfig>    
    
    <defaultDeviceConfig xsi:type="clicktocall:DeviceConfig">
        <enableImmediateClickToCall>false</enableImmediateClickToCall>
    </defaultDeviceConfig>
