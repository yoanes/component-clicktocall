<?xml version="1.0" encoding="UTF-8"?>
<core:deviceConfigs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:core="http://mobile.sensis.com.au/web/component/core/device"
    xmlns:clicktocall="http://mobile.sensis.com.au/web/component/clicktocall/device"
    xsi:schemaLocation="http://mobile.sensis.com.au/web/component/clicktocall/device au.com.sensis.mobile.web.component.clicktocall.device.xsd
